{
  "name": "angular2-universal-starter",
  "version": "2.0.0",
  "description": "Angular 2 Universal starter kit by @AngularClass",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal-starter.git"
  },
  "scripts": {
    "render": "npm run render:gulp",
    "render:gulp": "./node_modules/.bin/gulp prerender",
    "postinstall": "./node_modules/.bin/typings install",
    "watch": "./node_modules/.bin/webpack --watch",
    "build": "./node_modules/.bin/webpack && npm run render",
    "prestart": "npm run build",
    "start": "./node_modules/.bin/http-server dist --cors",
    "predebug": "npm run build",
    "debug": "node --debug-brk dist/server/bundle.js"
  },
  "license": "MIT",
  "contributors": [
    "AngularClass <hello@angularclass.com>",
    "PatrickJS <patrick@angularclass.com>",
    "Jeff Whelpley <jeff@gethuman.com>",
    "Jeff Cross <crossj@google.com>"
  ],
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "angular2-universal": "~0.104.5",
    "angular2-gulp-prerender": "^0.5.2",
    "css": "2.2.1",
    "es6-shim": "^0.33.3",
    "lodash": "^3.10.1",
    "parse5": "^1.5.0",
    "preboot": "~1.1.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "xhr2": "^0.1.3",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "http-server": "^0.8.5",
    "rimraf": "^2.5.0",
    "ts-loader": "^0.8.0",
    "ts-node": "^0.5.5",
    "typescript": "^1.7.5",
    "typings": "^0.6.8",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.7.1"
  }
}